// Place your settings in this file to overwrite default and user settings.
{
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/coverage_js": true,
    "**/coverage": true,
    "**/dist": true
  },
  "TodoParser": {
    "exclude": [
      "cpp",
      "c"
    ],
    "include": [
      "ts",
      "scss"
    ],
    "folderExclude": [
      "node_modules",
      ".vscode"
    ],
    "only": [],
    "markers": [
      "TODO:",
      "NOTE:",
      "REMINDER:"
    ],
    "autoDefaultMarkers": true
  }
}
